{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Spanright Calibrate",
  "version": "0.1.1",
  "identifier": "com.spanright.calibrate",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Spanright Calibrate",
        "width": 800,
        "height": 600
      }
    ]
  },
  "bundle": {
    "targets": ["msi", "nsis"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
